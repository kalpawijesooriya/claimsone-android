<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    android:clickable="true"
    android:gravity="center_vertical"
    android:onClick="onClick_keyboardClose"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/insuranceform_header"
        android:layout_width="@dimen/header_width"
        android:layout_height="@dimen/header_height"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/accidentform_top_header"
        android:orientation="vertical" >
    </LinearLayout>

    <LinearLayout
        android:id="@+id/insuranceform_body"
        android:layout_width="@dimen/formheader_width"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/insuranceform_body_LinearLayout1"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/insuranceform_LL1_height"
            android:layout_gravity="center"
            android:background="@drawable/insuranceform_body1"
            android:orientation="vertical" >

            <EditText
                android:id="@+id/nameofinsuredtext"
                android:layout_width="@dimen/editText_width"
                android:layout_height="@dimen/editText_height"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/insuranceform_nameofinsuredtext_marginTop"
                android:background="@drawable/fieldtextview"
                android:digits="@string/alphanumeric_specialcharacters"
                android:imeOptions="flagNavigateNext"
                android:inputType="textPersonName"
                android:lines="1"
                android:maxLength="100"
                android:maxLines="1"
                android:paddingLeft="@dimen/editText_paddingLeft"
                android:singleLine="true"
                android:textSize="@dimen/editText_textSize" >
            </EditText>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/insuranceform_body_LinearLayout2"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/insuranceform_LL2_height"
            android:layout_gravity="center"
            android:background="@drawable/insuranceform_body2"
            android:orientation="vertical" >

            <EditText
                android:id="@+id/contactnoofinsuredtext"
                android:layout_width="@dimen/editText_width"
                android:layout_height="@dimen/editText_height"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/insuranceform_contactnoofinsuredtext_marginTop"
                android:background="@drawable/fieldtextview"
                android:imeOptions="flagNavigateNext"
                android:inputType="phone"
                android:lines="1"
                android:maxLength="10"
                android:maxLines="1"
                android:paddingLeft="@dimen/editText_paddingLeft"
                android:singleLine="true"
                android:textSize="@dimen/editText_textSize" >
            </EditText>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/insuranceform_body_LinearLayout3"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/insuranceform_LL3_height"
            android:layout_gravity="center"
            android:background="@drawable/insuranceform_body3"
            android:orientation="vertical" >

            <EditText
                android:id="@+id/policycovernotenotext"
                android:layout_width="@dimen/editText_width"
                android:layout_height="@dimen/editText_height"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/insuranceform_policycovernotenotext_marginTop"
                android:background="@drawable/fieldtextview"
                android:digits="@string/alphanumeric_specialcharacters"
                android:imeOptions="flagNavigateNext"
                android:inputType="textPostalAddress"
                android:lines="1"
                android:maxLength="30"
                android:maxLines="1"
                android:paddingLeft="@dimen/editText_paddingLeft"
                android:singleLine="true"
                android:textSize="@dimen/editText_textSize" >
            </EditText>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/insuranceform_body_LinearLayout4"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/insuranceform_LL2_height"
            android:layout_gravity="center"
            android:background="@drawable/insuranceform_body4"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/linearLayout2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/insuranceform_linearLayout_marginLeft"
                android:layout_marginTop="@dimen/insuranceform_linearLayout2_marginTop"
                android:clickable="true"
                android:onClick="onClick_keyboardClose" >

                <EditText
                    android:id="@+id/datetextboxIF"
                    android:layout_width="@dimen/dateNmonth_textbox_width"
                    android:layout_height="@dimen/date_height"
                    android:background="@drawable/fieldtextview"
                    android:clickable="true"
                    android:contentDescription="DD"
                    android:hint="DD"
                    android:imeOptions="flagNavigateNext"
                    android:inputType="phone"
                    android:lines="1"
                    android:maxLength="2"
                    android:maxLines="1"
                    android:paddingLeft="@dimen/datetextbox_paddingleft"
                    android:saveEnabled="true"
                    android:selectAllOnFocus="true"
                    android:singleLine="true"
                    android:textSize="@dimen/editText_textSize" />

                <TextView
                    android:id="@+id/dateseparator1IF"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/date_height"
                    android:clickable="false"
                    android:text="/"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textColor="@color/black"
                    android:textSize="@dimen/editText_textSize" />

                <EditText
                    android:id="@+id/monthtextboxIF"
                    android:layout_width="@dimen/dateNmonth_textbox_width"
                    android:layout_height="@dimen/date_height"
                    android:layout_marginLeft="@dimen/monthtextbox_marginLeft"
                    android:background="@drawable/fieldtextview"
                    android:clickable="true"
                    android:contentDescription="MM"
                    android:hint="MM"
                    android:imeOptions="flagNavigateNext"
                    android:inputType="phone"
                    android:lines="1"
                    android:maxLength="2"
                    android:maxLines="1"
                    android:paddingLeft="@dimen/datetextbox_paddingleft"
                    android:saveEnabled="true"
                    android:selectAllOnFocus="true"
                    android:singleLine="true"
                    android:textSize="@dimen/editText_textSize" />

                <TextView
                    android:id="@+id/dateseparator2IF"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/date_height"
                    android:clickable="false"
                    android:text="/"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textColor="@color/black"
                    android:textSize="@dimen/editText_textSize" />

                <EditText
                    android:id="@+id/yeartextboxIF"
                    android:layout_width="@dimen/yeartextbox_width"
                    android:layout_height="@dimen/date_height"
                    android:layout_marginLeft="1dip"
                    android:background="@drawable/fieldtextview"
                    android:clickable="true"
                    android:contentDescription="YY"
                    android:hint="YY"
                    android:imeOptions="flagNavigateNext"
                    android:inputType="phone"
                    android:lines="1"
                    android:maxLength="2"
                    android:maxLines="1"
                    android:paddingLeft="@dimen/datetextbox_paddingleft"
                    android:saveEnabled="true"
                    android:selectAllOnFocus="true"
                    android:singleLine="true"
                    android:textSize="@dimen/editText_textSize" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/insuranceform_body_LinearLayout5"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/insuranceform_LL5_height"
            android:layout_gravity="center"
            android:background="@drawable/insuranceform_body5"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/linearLayout22"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/insuranceform_linearLayout_marginLeft"
                android:layout_marginTop="@dimen/insuranceform_linearLayout22_marginTop"
                android:clickable="true"
                android:onClick="onClick_keyboardClose" >

                <EditText
                    android:id="@+id/datetextboxIF2"
                    android:layout_width="@dimen/dateNmonth_textbox_width"
                    android:layout_height="@dimen/date_height"
                    android:background="@drawable/fieldtextview"
                    android:clickable="true"
                    android:contentDescription="DD"
                    android:hint="DD"
                    android:imeOptions="flagNavigateNext"
                    android:inputType="phone"
                    android:lines="1"
                    android:maxLength="2"
                    android:maxLines="1"
                    android:paddingLeft="@dimen/datetextbox_paddingleft"
                    android:saveEnabled="true"
                    android:selectAllOnFocus="true"
                    android:singleLine="true"
                    android:textSize="@dimen/editText_textSize" />

                <TextView
                    android:id="@+id/dateseparator1IF2"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/date_height"
                    android:clickable="false"
                    android:text="/"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textColor="@color/black"
                    android:textSize="@dimen/editText_textSize" />

                <EditText
                    android:id="@+id/monthtextboxIF2"
                    android:layout_width="@dimen/dateNmonth_textbox_width"
                    android:layout_height="@dimen/date_height"
                    android:layout_marginLeft="@dimen/monthtextbox_marginLeft"
                    android:background="@drawable/fieldtextview"
                    android:clickable="true"
                    android:contentDescription="MM"
                    android:hint="MM"
                    android:imeOptions="flagNavigateNext"
                    android:inputType="phone"
                    android:lines="1"
                    android:maxLength="2"
                    android:maxLines="1"
                    android:paddingLeft="@dimen/datetextbox_paddingleft"
                    android:saveEnabled="true"
                    android:selectAllOnFocus="true"
                    android:singleLine="true"
                    android:textSize="@dimen/editText_textSize" />

                <TextView
                    android:id="@+id/dateseparator2IF2"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/date_height"
                    android:clickable="false"
                    android:text="/"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textColor="@color/black"
                    android:textSize="@dimen/editText_textSize" />

                <EditText
                    android:id="@+id/yeartextboxIF2"
                    android:layout_width="@dimen/yeartextbox_width"
                    android:layout_height="@dimen/date_height"
                    android:layout_marginLeft="1dip"
                    android:background="@drawable/fieldtextview"
                    android:clickable="true"
                    android:contentDescription="YY"
                    android:hint="YY"
                    android:imeOptions="flagNavigateNext"
                    android:inputType="phone"
                    android:lines="1"
                    android:maxLength="2"
                    android:maxLines="1"
                    android:paddingLeft="@dimen/datetextbox_paddingleft"
                    android:saveEnabled="true"
                    android:selectAllOnFocus="true"
                    android:singleLine="true"
                    android:textSize="@dimen/editText_textSize" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/insuranceform_body_LinearLayout6"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/insuranceform_LL2_height"
            android:layout_gravity="center"
            android:background="@drawable/insuranceform_body6"
            android:orientation="vertical" >

            <EditText
                android:id="@+id/policycovernoteserialnotext"
                android:layout_width="@dimen/editText_width"
                android:layout_height="@dimen/editText_height"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/insuranceform_policycovernoteserialnotext_marginTop"
                android:background="@drawable/fieldtextview"
                android:digits="@string/alphanumeric_specialcharacters"
                android:imeOptions="flagNavigateNext"
                android:inputType="textPostalAddress"
                android:lines="1"
                android:maxLength="30"
                android:maxLines="1"
                android:paddingLeft="@dimen/editText_paddingLeft"
                android:singleLine="true"
                android:textSize="@dimen/editText_textSize" >
            </EditText>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/insuranceform_body_LinearLayout7"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/insuranceform_LL7_height"
            android:layout_gravity="center"
            android:background="@drawable/insuranceform_body7"
            android:orientation="vertical" >

            <EditText
                android:id="@+id/covernoteissuedbytext"
                android:layout_width="@dimen/editText_width"
                android:layout_height="@dimen/editText_height"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/insuranceform_covernoteissuedbytext_marginTop"
                android:background="@drawable/fieldtextview"
                android:digits="@string/alphanumeric_specialcharacters"
                android:imeOptions="flagNavigateNext"
                android:inputType="textPersonName"
                android:lines="1"
                android:maxLength="50"
                android:maxLines="1"
                android:paddingLeft="@dimen/editText_paddingLeft"
                android:singleLine="true"
                android:textSize="@dimen/editText_textSize" >
            </EditText>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/insuranceform_body_LinearLayout8"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/insuranceform_LL8_height"
            android:layout_gravity="center"
            android:background="@drawable/insuranceform_body8"
            android:orientation="vertical" >

            <EditText
                android:id="@+id/reasonsforissuingcovernotetext"
                android:layout_width="@dimen/editText_width"
                android:layout_height="@dimen/editText_height"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/insuranceform_reasonsforissuingcovernotetext_marginTop"
                android:background="@drawable/fieldtextview"
                android:digits="@string/alphanumeric_specialcharacters"
                android:imeOptions="actionDone"
                android:inputType="textShortMessage"
                android:lines="1"
                android:maxLength="150"
                android:maxLines="1"
                android:paddingLeft="@dimen/editText_paddingLeft"
                android:singleLine="true"
                android:textSize="@dimen/editText_textSize" >
            </EditText>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout21"
        android:layout_width="@dimen/header_width"
        android:layout_height="@dimen/bottom_button_bar_height"
        android:layout_gravity="center"
        android:layout_marginTop="@dimen/bottom_button_marginTop"
        android:background="@drawable/bottom_button_bar"
        android:clickable="true"
        android:onClick="onClick_keyboardClose" >

        <Button
            android:id="@+id/InsuranceformBackButton"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:layout_marginLeft="@dimen/buttons_margin"
            android:layout_marginTop="@dimen/buttons_margin"
            android:background="@drawable/back_button_xml"
            android:clickable="true"
            android:onClick="on_click_back_button" >
        </Button>

        <Button
            android:id="@+id/InsuranceformNextButton"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:layout_marginLeft="@dimen/button2_marginLeft"
            android:layout_marginTop="@dimen/buttons_margin"
            android:background="@drawable/next_button_xml"
            android:clickable="true"
            android:onClick="on_click_next_button" >
        </Button>
    </LinearLayout>

</LinearLayout>